<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
    <h3>Members List</h3>
    <h4><a href="/member/add">Add Members</a></h4>
    <table>
            <thead>
                <tr>
                    <th>No</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Option</th>
                </tr>
            </thead>
        <tbody>
            <% data.forEach((datum,index) => { %>
                <tr>
                    <td><%= index+1 %></td>
                    <td><%= datum.name %></td>
                    <td><%= datum.email %></td>
                    <td><%= datum.phone %></td>
                    <td><a href="/member/edit/<%= datum.id %>"><button type="button">Update</button></a></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
    <a href="/">Home</a>
</body>
</html>